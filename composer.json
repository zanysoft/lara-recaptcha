{
  "name": "zanysoft/laravel-recaptcha",
  "description": "Advanced Google reCAPTCHA package for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "ZanySoft",
      "email": "info@zanysoft.net"
    }
  ],
  "keywords": [
    "laravel",
